# ~/.config/rocket-mav/rate.env or /etc/rocket-mav/rate.env
# precedence: process env > user file > system file > code default

# serial baud
ROCKET_MAV_SCAN_SERIAL_BAUD=921600
ROCKET_MAV_TOOLS_SERIAL_BAUD=921600

# scan / param_fetch timing
ROCKET_MAV_SCAN_DURATION_SEC=5
ROCKET_MAV_SCAN_PARAM_TIMEOUT_SEC=10
ROCKET_MAV_PARAM_FETCH_TIMEOUT_SEC=3
ROCKET_MAV_PARAM_FETCH_RETRIES=3

# app loop rates
ROCKET_MAV_SERVO_LOOP_HZ=50
ROCKET_MAV_MOTOR_LOOP_HZ=50
ROCKET_MAV_STREAM_ODOM_RATE_HZ=100
ROCKET_MAV_STREAM_CMD_RATE_HZ=100
ROCKET_MAV_STREAM_CMD_IDLE_RATE_HZ=10
ROCKET_MAV_STREAM_CMD_ACTIVE_RATE_HZ=200
ROCKET_MAV_STREAM_CMD_ACTIVE_HOLD_SEC=0.5
ROCKET_MAV_STREAM_CMD_MANUAL_KEEPALIVE_HZ=5
ROCKET_MAV_STREAM_CMD_RX_BURST_MAX=200
ROCKET_MAV_STREAM_CMD_RX_PARSE_HZ=200

# test app output limits
ROCKET_MAV_STREAM_ODOM_TEST_CONSOLE_BPS=5760
ROCKET_MAV_STREAM_ODOM_TEST_PRINT_HZ=20
ROCKET_MAV_STREAM_CMD_TEST_CONSOLE_BPS=5760

# monitoring timing
ROCKET_MAV_MONITORING_QUERY_MAX_WAIT_SEC=0.25
ROCKET_MAV_MONITORING_QUERY_SELECT_TIMEOUT_SEC=0.05
ROCKET_MAV_MONITORINGD_SELECT_TIMEOUT_SEC=0.2
ROCKET_MAV_MONITORINGD_WRITE_INTERVAL_SEC=0.5
ROCKET_MAV_MONITORINGD_TIME_SYNC_THRESHOLD_SEC=60
ROCKET_MAV_MONITORINGD_TIME_SYNC_RETRY_INTERVAL_SEC=10
